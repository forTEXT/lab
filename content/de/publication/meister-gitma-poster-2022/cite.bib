@inproceedings{meister_gitma-poster_2022,
 abstract = {Seit CATMA Version 6.0 kann auf Projektdaten in Form von Git Repositorien zugegriffen werden. Im Fachbereich für Digital Philology an der TU Darmstadt ist außerdem eine Python-Bibliothek entstanden, die einen einfachen Zugriff auf diese zulässt. Die Bilbiothek ermöglicht die Weiterverarbeitung der Annotationen mit gängigen Python Datascience-Tools. Das Poster wird eine Art Bedienungsanleitung für die Nutzung des CATMA Git Access und der Python-Bibliothek sein. Ein Beitrag zur 8. Tagung des Verbands "Digital Humanities im deutschsprachigen Raum" - DHd 2022 Kulturen des digitalen Gedächtnisses.},
 address = {Potsdam},
 author = {Meister, Malte and Vauth, Michael and Gerstorfer, Dominik},
